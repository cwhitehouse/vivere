<div
  *lists-shuffle*
  #items='[{"id":1,"name":"Item # 1"},{"id":2,"name":"Item # 2"},{"id":3,"name":"Item # 3"},{"id":4,"name":"Item # 4"},{"id":5,"name":"Item # 5"}]'
>
  <button
    @click="shuffleItems"
    class="button"
  >Shuffle Items</button>
  <ul class="mt-6">
    <li
      *for="item of shuffledItems"
      :text="item.name"
    />
  </ul>
</div>