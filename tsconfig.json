{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [ "dom", "esnext" ],
    "module": "es2015",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "rootDir": ".",
    "strict": false,
    "target": "es6",
    "noEmit": true,
    "removeComments": true,
    "paths": {
      "@jsep-plugin/template": ["./src/types/jsep-template"]
    }
  },
  "exclude": [ "dist", "temp", "_examples" ]
}
